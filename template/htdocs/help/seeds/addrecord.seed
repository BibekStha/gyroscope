<?php

function add#record#(){
	$#dispfield#=GETSTR('#dispfield#');
	
	global $db;
	
	$query="insert into #tablename# (#dispfield#) values ('$#dispfield#') ";
	$rs=sql_query($query,$db);
	$#primarykey#=sql_insert_id($db,$rs);

	logaction("added #c_record# #$#primarykey# <u>$#dispfield#</u>",array('#primarykey#'=>$#primarykey#,'#dispfield#'=>"$#dispfield#"));
	
	echo $#primarykey#; die();
}
